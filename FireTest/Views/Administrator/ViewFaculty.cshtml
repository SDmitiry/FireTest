@model FireTest.Models.FacultyViewQualifications
@{
    ViewBag.Title = "Посмотреть факультет";
}
@Styles.Render("~/Content/administrator")
<div class="row center"><h3>@Html.DisplayTextFor(m => m.Name)</h3></div>
<div class="row">
    @{
        for (int i = 0; i < Model.Bachelor + Model.Master; i++)
        {
            <div class="col-2 center facultyQualifications">
                <img class="" src="~/Images/Qualifications/@Model.LevelsPictures[i]" />
                <p>@Html.DisplayTextFor(m => m.LevelsName[i])</p>
            </div>
        }
    }
</div>